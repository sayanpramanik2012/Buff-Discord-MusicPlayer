version: "3.8"

services:
  discord-bot:
    image: sayanpramanik/buff-discord-musicplayer:latest
    container_name: DiscordMusicBotBUFF
    ports:
      - "80:5000"
    environment:
      BOT_TOKEN: "BOT_TOKEN_GOES_HERE"
      PREFIX: "#"
      SPOTIFY_CLIENT_ID: "SPOTIFY_CLIENT_ID_GOES_HERE"
      SPOTIFY_CLIENT_SECRET: "SPOTIFY_CLIENT_SECRET_GOES_HERE"
      YOUTUBE_API_KEY: "YOUTUBE_API_KEY_GOES_HERE"
